/* #define ENABLESHARED */
#include "../../config/lclib.tmpl"

XCOMM #
XCOMM # Define the name of the library.
XCOMM #
INSTALLDIR = ../../lib
LIBNAME   = GENLIBHG
XCOMM #
XCOMM # Routines to compile.
XCOMM #
OBJS = ampff.o \
		ampha.o \
		amphhc.o \
		ampzh.o \
		ewinit.o \
		gma2ff.o \
		gmh2ud.o \
		gmt2hb.o \
		inhgcn.o \
		mhiggs.o

XCOMM #--
XCOMM #  Library
XCOMM #--
ALLOBJ = $(OBJS)
#ifdef DOSHRLIB
SharedLibraryTarget($(LIBNAME),$(SOREV),$(ALLOBJ),.,.)
InstallSharedLibrary($(LIBNAME),$(SOREV),$(INSTALLDIR))
#endif
NormalLibraryTarget($(LIBNAME), $(ALLOBJ) )
InstallLibrary($(LIBNAME),$(INSTALLDIR))

distclean: clean
	rm -f Makefile

